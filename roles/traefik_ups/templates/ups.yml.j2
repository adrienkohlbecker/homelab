http:
  routers:
    ups:
      entryPoints:
        - websecure
      service: ups
      rule: Host(`ups.{{ domain }}`) || Host(`ups.{{ inventory_hostname }}.{{ domain }}`)
      tls: {}
  services:
    ups:
      loadBalancer:
        servers:
          - url: 'https://{{ external_ips.eaton }}:443/'
