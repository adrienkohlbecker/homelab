http:
  routers:
    unifi:
      entryPoints:
        - websecure
      service: unifi
      rule: Host(`unifi.{{ domain }}`) || Host(`unifi.{{ inventory_hostname }}.{{ domain }}`)
      tls: {}
  services:
    unifi:
      loadBalancer:
        servers:
          - url: 'https://{{ external_ips.udm }}:443/'
