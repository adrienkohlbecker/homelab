entryPoints:
  web:
    address: ':80'
    http:
      redirections:
        entryPoint:
          to: websecure
          scheme: https
  websecure:
    address: ':443'
    http:
      tls: {}

serversTransport:
  insecureSkipVerify: true

providers:
  docker:
    exposedByDefault: false
    network: traefik
  file:
    directory: /etc/traefik/dynamic.d
    watch: true

ping:
  entryPoint: websecure
  manualRouting: true
api: {}
accessLog: {}
log:
  level: INFO
